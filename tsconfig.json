{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@views/*":["app/views/*"],
      "@components/*":["app/components/*"],
      "@utils/*":["app/utils/*"],
      "@constants/*":["app/constants/*"],
      "@styles/*":["app/styles/*"],
      "@assets/*":["app/assets/*"],
      "@navigation/*":["app/navigation/*"],
      "@screens/*":["app/screens/*"],
      "@hooks/*":["app/hooks/*"],
      "@services/*":["app/services/*"],
      "@store/*":["app/store/*"],
      "@app/*":["app/*"],
      
    }
  }
}
